{
  "name": "nature-immersive-article-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "rollup -c -w",
    "clean": "rm public/build/* && rm ssr/*",
    "build": "rollup -c",
    "build:ssr-data": "node scripts/render-data.js",
    "build:ssr-js": "rollup -c rollup.config.ssr.js",
    "build:tracking-js": "rollup -c rollup.config.tracking.js",
    "build:ssr-html": "node ./scripts/render-static-html.js",
    "build:ssr-css": "node ./scripts/render-static-css.js",
    "ssr": "npm run clean && npm run build:ssr-data && npm run build && npm run build:ssr-js && npm run build:ssr-html && npm run build:ssr-css",
    "prettier": "prettier --write --plugin-search-dir=. src/**/* scripts/**/*",
    "stylelint": "stylelint --fix src/**/* static/css/**/*.css",
    "eslint": "eslint 'scripts/**/*.js' 'src/**/*.{js,svelte}'",
    "start": "sirv public"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "archieml": "^0.4.2",
    "autoprefixer": "^10.0.4",
    "creditdesign-svelte-components": "^0.1.1",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "gray-matter": "^4.0.2",
    "htmltidy2": "^0.3.0",
    "marked": "^1.2.0",
    "nature-immersive-svelte-components": "^0.3.17",
    "postcss": "^8.1.14",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^0.4.5",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.6.1"
  },
  "dependencies": {}
}
